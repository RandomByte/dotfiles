gf() { git fetch origin $1 && git checkout FETCH_HEAD ; }
gp() { git push -u origin HEAD:refs/for/$1 ; }

# Get to the top of a git tree
cdp () {
  TEMP_PWD=`pwd`
  while ! [ -d .git ]; do
    cd ..
  done
  OLDPWD=$TEMP_PWD
}

ssh-pw () {
  ssh -o PubkeyAuthentication=no $1
}

keygen () {
  ssh-keygen -t rsa -b 4096 -C $(hostname)
}
